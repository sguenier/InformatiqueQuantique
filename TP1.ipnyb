from qiskit import *

quantCirc = QuantumCircuit(3, 3)

quantCirch([0, 2])
quantCirc.cx(0, 1) 

quantCirc.barrier()

quantCirc.cx(2, 1)

qc.x(2)

qc.cx(2, 0)

qc.x(2)

qc.barrier()

qc.swap(0, 1)

qc.x(1)

qc.cx(2, 1)

qc.x([0, 1, 2])

qc.cx(2, 0)

qc.x(2)

qc.measure([0, 1, 2], [0, 1, 2])

qc.draw()
