from qiskit import *

quantCirc = QuantumCircuit(3, 3)

quantCirch([0, 2])
quantCirc.cx(0, 1) 

quantCirc.barrier()

quantCirc.cx(2, 1)

quantCirc.x(2)

quantCirc.cx(2, 0)

quantCirc.x(2)

quantCirc.barrier()

quantCirc.swap(0, 1)

quantCirc.x(1)

quantCirc.cx(2, 1)

quantCirc.x([0, 1, 2])

quantCirc.cx(2, 0)

quantCirc.x(2)

quantCirc.measure([0, 1, 2], [0, 1, 2])

quantCirc.draw()
